<!DOCTYPE html>
<html>
  <head>
    <title>Workspace</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>


    <!-- <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet" id="bootstrap-css"> -->
    <!-- <script src="//code.jquery.com/jquery-1.11.1.min.js"></script> -->
  </head>
  <body>
    <nav class="navbar navbar-default" style="margin-top: 30px;">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="<%=root_path%>">Day In The Life Of</a>
        </div>
        <ul class="nav navbar-nav">
          <li><a href="/videomasterindex/search">Search</a></li>
          <li>
            <% if @beenhere %>
              <a href='/videomasterindex/getpaid' data-step='1' data-intro='Learn How to Get Paid By Us!' data-position="right"> Get Paid </a> 
            <% else %>
              <a href='/videomasterindex/getpaid'> Get Paid </a> 
            <% end %>
          </li>
          <li><a href="/videomasterindex/contactus">Contact Me</a></li>
          <li><a href="/videos/new">Videos New</a></li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <% if user_signed_in? %>
            <li><%= link_to "Sign out", destroy_user_session_path, :method => :delete %></li>
          <% else %>
            <li><a href="/users/sign_up">Sign Up</a></li>
            <li><a href="/users/sign_in">Sign In</a></li>
          <% end %>
        </ul>
      </div>
    </nav>
    <div class="container">
      <% if notice %>
        <div class="alert alert-success alert-dismissible">
          <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
          <%= notice %>
        </div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>